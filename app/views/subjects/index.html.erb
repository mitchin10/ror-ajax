<h1>Subjects</h1>
<div class="actions">
  <%= link_to 'New Subject', new_subject_path, remote: true, class: 'btn btn-primary btn-sm' %>
</div>

<p id="notice"><%= notice %></p>

<table class="table-responsive" id="subjects">
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= render @subjects %>
  </tbody>
</table>

<%= render 'dialog' %>
